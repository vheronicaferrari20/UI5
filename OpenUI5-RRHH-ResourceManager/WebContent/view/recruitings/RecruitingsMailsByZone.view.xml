<core:View
    xmlns:core="sap.ui.core"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    controllerName="App.controller.recruitings.RecruitingsMailsByZone"
    xmlns:l="sap.ui.layout.form"
    xmlns:html="http://www.w3.org/1999/xhtml"
    height="100%">


    <ScrollContainer
        class="centerAuto"
        horizontal="false"
        vertical="true"
        height="98%"
        width="98%">

        <ObjectHeader
            class="header-page"
            title="Mails By Zone"
            titleLevel="H1"
            condensed="true"
            icon="sap-icon://email"
            backgroundDesign="Transparent"
            responsive="true">

            <headerContainer>

                <IconTabBar
                    id="iconTabBar"
                    select="handleIconTabBarSelect"
                    applyContentPadding="false"
                    expandable="false">
                    <items>
                        <IconTabFilter
                                key="MailsByZone"
                                icon="sap-icon://list">

                            <mvc:XMLView viewName="App.view.recruitings.RecruitingsMailsByZoneList"/>

                        </IconTabFilter>
                    </items>
                </IconTabBar>

            </headerContainer>

        </ObjectHeader>

    </ScrollContainer>

</core:View>